{
    "name": "pbald/geophp-bundle",
    "license": "GPL-3",
    "type": "library",
    "description": "GeoPHP Symfony2 Bundle",
    "keywords": ["parser", "lexer", "string", "text", "geometry", "geography", "spatial", "geo", "coordinate"],
    "require": {
        "php":            ">=5.3.3",
        "ext-SPL":        "*"
    },
    "require-dev": {
        "phpunit/phpunit":               ">=4.3"
    },
    "autoload": {
        "psr-4": {
            "GeoPHP\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeoPHP\\Tests\\": "tests/"
        }
    }
}
